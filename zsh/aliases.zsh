alias startserver='uvicorn app.main:app --reload'

# Go Related
alias gogo="cd ~/Documents/programming/go/"
# Terminal
alias ls="exa"
alias sl="exa"

# Git
alias ga="git add ."
alias gac="git add .; git commit"
alias gc="git commit"

# npm/react related
alias rd='npm run dev'

# C++
alias g11="g++ -std=c++11"
